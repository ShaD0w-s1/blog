{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:vue/essential", "airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": ["vue", "@typescript-eslint", "prettier"],
    "rules": {
        "quotes": ["error", "double"]
    }
}
